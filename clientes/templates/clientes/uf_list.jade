extends base.jade

block content
    h1 Unidade Federativa
    table
        thead
            td Nome
            td Editar
            td Excluir
        tbody
            each uf in object_list
                tr
                    td #{uf.nome}
                    td
                        a(href="{% url 'clientes:uf_edit' uf.id %}") edit #{' '}
                    td
                        a(href="{% url 'clientes:uf_delete' uf.id %}") delete #{' '}

    a(href="{% url 'clientes:uf_new' %}") New

